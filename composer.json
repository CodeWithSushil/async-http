  "funding": [
  {
    "type": "github",
    "url": "https://github.com/sponsors/CodeWithSushil"
  }
],
  "scripts": {
      "test": "vendor/bin/phpunit"
  }
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
